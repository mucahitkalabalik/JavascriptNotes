// Ä°ki tarih arasÄ±ndaki sÃ¼reyi hesaplayan fonksiyon
let firstTime = (dateStart, dateCreated) => {
  // SeÃ§ili tarih verisini sÄ±fÄ±rlar
  selectedHistory.value = null

  // Ä°lk tarih nesnesini oluÅŸturur (el baÅŸlama zamanÄ±)
  const date1 = new Date(dateStart)

  // Ä°kinci tarih nesnesini oluÅŸturur (iÅŸlem yaratÄ±lma zamanÄ±)
  const date2 = new Date(dateCreated)

  // Ä°ki tarih arasÄ±ndaki farkÄ± hesaplar (milisaniye cinsinden)
  let timeDifference = date2.getTime() - date1.getTime()

  // FarkÄ± saniye cinsine Ã§evirir
  var seconds = new Date(timeDifference).getTime() / 1000

  // Hesaplanan sÃ¼reyi dÃ¶ndÃ¼rÃ¼r
  return seconds
}

